package tv.snorkl.utils.bitmap {	import flash.display.Sprite;	import flash.display.Bitmap;				public class BitmapTile extends Sprite{		private var _originX:Number;		private var _originY:Number;						public function BitmapTile(image:Bitmap, x:Number, y:Number) {			this._originX = x;			this._originY = y;			this.addChild(image);											}				public function get originX():Number{			return _originX;			}					public function get originY():Number{			return _originY;			}		}	}